{
  "name": "nano-pulse",
  "version": "1.0.0",
  "description": "This system will handle real-time data processing for:",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/app-express/index.ts",
    "build": "tsc",
    "setup-apikey": "ts-node scripts/setup-test-apikey.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stress": "jest webhook.routes.stress.test.ts --runInBand",
    "kafka:create-topics": "chmod +x ./scripts/kafka_topics_create.sh && ./scripts/kafka_topics_create.sh",
    "kafka:list-topics": "kafka-topics --list --bootstrap-server localhost:9092",
    "kafka:delete-topics": "kafka-topics --bootstrap-server localhost:9092 --delete --topic webhook:queue:medium,webhook:queue:low --if-exists"
  },
  "keywords": [
    "webhook",
    "webhook-service",
    "webhook-consumer",
    "webhook-producer",
    "webhook-queue",
    "redis",
    "kafka",
    "nodejs",
    "typescript",
    "express",
    "mongoose",
    "ioredis"
  ],
  "author": "Thuta Sann",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.5.1",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.5",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.11.0",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "socket.io-redis": "^6.1.1",
    "zod": "^3.24.2"
  }
}
